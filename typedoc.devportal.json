{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/subscriber.ts", "src/subscriptions.ts", "src/types/*.ts"],
  "exclude": ["src/**/*.spec.ts", "tests/**/*.*", "examples/**/*.*"],
  "entryPointStrategy": "expand",
  "outputs": [{ "name": "markdown", "path": ".devportal/api" }],
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter", "./frontmatter-plugin.mjs"],
  "cleanOutputDir": true,
  "gitRevision": "main",
  "tsconfig": "tsconfig.build.json",
  "highlightLanguages": ["typescript", "python", "json", "bash", "js", "yaml"],
  "outputFileStrategy": "modules",
  "enumMembersFormat": "table",
  "parametersFormat": "table",
  "propertiesFormat": "table",
  "typeDeclarationFormat": "table",
  "indexFormat": "table",
  "includeHierarchySummary": false,
  "pageTitleTemplates": {
    "index": "{projectName}",
    "member": "{group} - `{rawName}`",
    "module": "{name}"
  }
}
