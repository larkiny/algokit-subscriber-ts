{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts", "src/types/*.ts"],
  "exclude": ["src/**/*.spec.ts", "tests/**/*.*", "examples/**/*.*"],
  "entryPointStrategy": "expand",
  "outputs": [{ "name": "markdown", "path": ".devportal/api" }],
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter", "./frontmatter-plugin.mjs"],
  "cleanOutputDir": true,
  "gitRevision": "main",
  "tsconfig": "tsconfig.build.json",
  "highlightLanguages": ["typescript", "python", "json", "bash", "js", "yaml"],
  "outputFileStrategy": "modules",
  "includeHierarchySummary": false
}
